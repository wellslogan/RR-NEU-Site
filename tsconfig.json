{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@models": ["src/_shared/models"],
      "@models/*": ["src/_shared/models/*"],
      "@shared": ["src/_shared"],
      "@shared/*": ["src/_shared/*"],
      "@app/*": ["src/*"]
    },
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "allowJs": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}
